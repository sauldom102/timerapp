{
  "name": "timerapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "husky install && patch-package",
    "bump": "sh ./scripts/bump-version.sh",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:prod": "ENVFILE=.env.prod yarn ios",
    "start": "react-native start",
    "test": "TZ=UTC jest --runInBand --forceExit",
    "test-detox": "detox test --configuration ios.sim.debug",
    "lint": "tsc --noEmit && eslint --cache 'src/**/*.ts?(x)'",
    "lint:fix": "yarn lint --fix",
    "lint:staged": "yarn lint-staged",
    "open:xcode": "open ios/rnapp.xcworkspace/",
    "sb:on": "exec ./scripts/storybook-enabler.sh on",
    "sb:off": "exec ./scripts/storybook-enabler.sh off",
    "sb:getstories": "sb-rn-get-stories",
    "sb:watcher": "sb-rn-watcher",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.5",
    "@hookform/resolvers": "2.9.11",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/datetimepicker": "^6.4.2",
    "@react-native-community/hooks": "^3.0.0",
    "@react-native-community/picker": "1.6.0",
    "@react-navigation/bottom-tabs": "6.5.4",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "6.9.9",
    "@tanstack/react-query": "^4.28.0",
    "date-fns": "^2.29.3",
    "expo": "47.0.13",
    "expo-image": "^1.0.0",
    "expo-linear-gradient": "~12.0.1",
    "expo-splash-screen": "~0.17.5",
    "i18next": "21.8.13",
    "intl-pluralrules": "1.3.1",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.43.1",
    "react-i18next": "11.18.0",
    "react-native": "^0.71.0",
    "react-native-animated-spinkit": "^1.5.2",
    "react-native-asset": "^2.1.0",
    "react-native-circular-progress": "^1.3.8",
    "react-native-config": "1.5.0",
    "react-native-date-picker": "^4.2.10",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-mmkv": "2.5.1",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "^2.14.1",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.19.0",
    "react-native-svg": "^13.7.0",
    "react-native-toast-message": "^2.1.6",
    "styled-components": "5.3.6",
    "uuid": "3.4.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@7nohe/openapi-react-query-codegen": "^0.3.1",
    "@babel/core": "7.18.6",
    "@babel/preset-env": "7.1.6",
    "@babel/runtime": "7.18.6",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@react-native-community/eslint-config": "2.0.0",
    "@react-native-community/slider": "4.4.2",
    "@rnx-kit/align-deps": "^2.2.0",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-ondevice-actions": "6.0.1-beta.6",
    "@storybook/addon-ondevice-backgrounds": "6.0.1-beta.6",
    "@storybook/addon-ondevice-controls": "6.0.1-beta.6",
    "@storybook/addon-ondevice-notes": "6.0.1-beta.6",
    "@storybook/react-native": "6.0.1-beta.6",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/react-native": "^12.0.1",
    "@types/enzyme": "3.10.12",
    "@types/jest": "29.0.3",
    "@types/lodash": "4.14.191",
    "@types/react-native": "0.67.11",
    "@types/react-test-renderer": "17.0.2",
    "@types/styled-components-react-native": "5.2.1",
    "@types/uuid": "3.4.0",
    "@typescript-eslint/eslint-plugin": "5.30.4",
    "@typescript-eslint/parser": "5.30.4",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "babel-jest": "29.0.3",
    "babel-plugin-module-resolver": "4.1.0",
    "detox": "^20.6.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "jest": "^29.2.1",
    "lint-staged": "13.0.3",
    "metro-react-native-babel-preset": "^0.73.7",
    "patch-package": "6.4.7",
    "react-is": "17.0.2",
    "react-native-dotenv": "3.3.1",
    "react-native-flipper": "0.153.0",
    "react-native-mmkv-flipper-plugin": "1.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.7.4"
  },
  "resolutions": {
    "@types/react": "17.0.47"
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.71"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "datetime-picker",
        "gestures",
        "jest",
        "navigation/native",
        "react",
        "react-dom",
        "react-test-renderer",
        "safe-area",
        "screens",
        "storage",
        "svg"
      ]
    }
  }
}
